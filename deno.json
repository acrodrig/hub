{
  "name": "@acr/hub",
  "version": "0.1.6",
  "license": "MIT",
  "exports": "./mod.ts",
  "fmt": {
    "lineWidth": 180,
    "include": [
      "src/",
      "test/"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1",
    "@std/async": "jsr:@std/async@^1",
    "@std/fmt": "jsr:@std/fmt@^1"
  },
  "lock": false,
  "tasks": {
    "check": "deno check **/*.ts && deno lint && deno fmt --check",
    "lint": "deno lint src test",
    "release": "release",
    "test": "deno test -A"
  }
}
